    return () => {
      /* istanbul ignore next */
      cov_qlu281n4p().f[16]++;
      cov_qlu281n4p().s[83]++;
      document.removeEventListener("keydown", handleKeyDown);
      /* istanbul ignore next */
      cov_qlu281n4p().s[84]++;
      document.removeEventListener("mousedown", handleMouseDown);
    };
  }, [enableKeyboardNavigation, focusOnElement]);
  // Announce page changes for screen readers
  /* istanbul ignore next */
  cov_qlu281n4p().s[85]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_qlu281n4p().f[17]++;
    cov_qlu281n4p().s[86]++;
    if (!announcePageChanges) {
      /* istanbul ignore next */
      cov_qlu281n4p().b[18][0]++;
      cov_qlu281n4p().s[87]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_qlu281n4p().b[18][1]++;
    }
    cov_qlu281n4p().s[88]++;
    const announcePageTitle = () => {
      /* istanbul ignore next */
      cov_qlu281n4p().f[18]++;
      const title =
      /* istanbul ignore next */
      (cov_qlu281n4p().s[89]++, document.title);
      /* istanbul ignore next */
      cov_qlu281n4p().s[90]++;
      if (title) {
        /* istanbul ignore next */
        cov_qlu281n4p().b[19][0]++;
        cov_qlu281n4p().s[91]++;
        announceToScreenReader(`Page loaded: ${title}`);
      } else
      /* istanbul ignore next */
      {
        cov_qlu281n4p().b[19][1]++;
      }
    };
    // Announce initial page load
    /* istanbul ignore next */
    cov_qlu281n4p().s[92]++;
    setTimeout(announcePageTitle, 100);
    // Listen for navigation changes (for SPA routing)
    /* istanbul ignore next */
    cov_qlu281n4p().s[93]++;
    const handlePopState = () => {
      /* istanbul ignore next */
      cov_qlu281n4p().f[19]++;
      cov_qlu281n4p().s[94]++;
      setTimeout(announcePageTitle, 100);
    };
    /* istanbul ignore next */
    cov_qlu281n4p().s[95]++;
    window.addEventListener("popstate", handlePopState);
    /* istanbul ignore next */
    cov_qlu281n4p().s[96]++;
    return () => {
      /* istanbul ignore next */
      cov_qlu281n4p().f[20]++;
      cov_qlu281n4p().s[97]++;
      return window.removeEventListener("popstate", handlePopState);
    };
  }, [announcePageChanges, announceToScreenReader]);
  /* istanbul ignore next */
  cov_qlu281n4p().s[98]++;
  return {
    isReducedMotion,
    announceToScreenReader,
    focusOnElement,
    trapFocus,
    getSkipLinks
  };
}
