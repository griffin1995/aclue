            className: "flex items-center gap-2 mb-3",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
              className: "text-xl font-bold text-gray-900",
              children: formatPrice(product.price, product.currency)
            }),
            /* istanbul ignore next */
            (cov_1i38zm6wio().b[54][0]++, product.original_price) &&
            /* istanbul ignore next */
            (cov_1i38zm6wio().b[54][1]++, product.original_price > product.price) &&
            /* istanbul ignore next */
            (cov_1i38zm6wio().b[54][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
