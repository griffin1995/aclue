    {
      cov_12n9nmp530().b[16][1]++;
    }
    cov_12n9nmp530().s[79]++;
    const updateNetworkStatus = () => {
      /* istanbul ignore next */
      cov_12n9nmp530().f[23]++;
      cov_12n9nmp530().s[80]++;
      setIsOnline(navigator.onLine);
      // Get connection type if available
      const connection =
      /* istanbul ignore next */
      (cov_12n9nmp530().s[81]++,
      /* istanbul ignore next */
      (cov_12n9nmp530().b[17][0]++, navigator.connection) ||
      /* istanbul ignore next */
      (cov_12n9nmp530().b[17][1]++, navigator.mozConnection) ||
      /* istanbul ignore next */
      (cov_12n9nmp530().b[17][2]++, navigator.webkitConnection));
      /* istanbul ignore next */
      cov_12n9nmp530().s[82]++;
      if (connection) {
        /* istanbul ignore next */
        cov_12n9nmp530().b[18][0]++;
        cov_12n9nmp530().s[83]++;
        setConnectionType(
        /* istanbul ignore next */
        (cov_12n9nmp530().b[19][0]++, connection.effectiveType) ||
        /* istanbul ignore next */
        (cov_12n9nmp530().b[19][1]++, connection.type) ||
        /* istanbul ignore next */
        (cov_12n9nmp530().b[19][2]++, "unknown"));
      } else
      /* istanbul ignore next */
      {
        cov_12n9nmp530().b[18][1]++;
      }
    };
    /* istanbul ignore next */
    cov_12n9nmp530().s[84]++;
    updateNetworkStatus();
    /* istanbul ignore next */
    cov_12n9nmp530().s[85]++;
    window.addEventListener("online", updateNetworkStatus);
    /* istanbul ignore next */
    cov_12n9nmp530().s[86]++;
    window.addEventListener("offline", updateNetworkStatus);
    /* istanbul ignore next */
    cov_12n9nmp530().s[87]++;
    return () => {
      /* istanbul ignore next */
      cov_12n9nmp530().f[24]++;
      cov_12n9nmp530().s[88]++;
      window.removeEventListener("online", updateNetworkStatus);
      /* istanbul ignore next */
      cov_12n9nmp530().s[89]++;
      window.removeEventListener("offline", updateNetworkStatus);
    };
  }, []);
  /* istanbul ignore next */
  cov_12n9nmp530().s[90]++;
  return {
    isOnline,
    connectionType
  };
};
