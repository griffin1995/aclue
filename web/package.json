{
  "name": "aclue-web",
  "version": "1.0.1",
  "description": "Aclue Web Application - A data-led insight layer that transforms how gifts are chosen",
  "_rebrandingNote": "Package renamed from aclue-web/aclue-web to aclue-web during August 2025 rebranding",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:optimized": "next build && node scripts/build-optimizer.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "export": "next export",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:ci": "jest --ci --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern='.*\\.test\\.(ts|tsx)$' --passWithNoTests",
    "test:integration": "jest --testPathPattern='.*\\.integration\\.test\\.(ts|tsx)$' --passWithNoTests",
    "test:component": "jest --testPathPattern='components/.*\\.test\\.(ts|tsx)$' --passWithNoTests",
    "vercel-build": "next build",
    "deploy": "vercel --prod",
    "pages:deploy": "next build",
    "optimize": "node scripts/build-optimizer.js",
    "perf": "npm run build:optimized",
    "perf:test": "node scripts/performance-test.js",
    "perf:test:mobile": "node scripts/performance-test.js --device mobile --throttle 4g",
    "perf:test:slow": "node scripts/performance-test.js --throttle 3g",
    "perf:analyze": "npm run build:analyze && npm run perf:test",
    "perf:report": "npm run optimize && npm run perf:test --runs 5",
    "performance:baseline": "node scripts/performance-baseline.js",
    "app-router:test": "jest --config jest.app-router.config.js",
    "app-router:build": "NEXT_PUBLIC_APP_ROUTER_ENABLED=true next build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@sentry/react": "^10.12.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-cookie": "^3.0.0",
    "@types/node": "^20.0.0",
    "@types/react-dom": "^18.2.0",
    "@use-gesture/react": "^10.2.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "critters": "^0.0.23",
    "framer-motion": "^10.16.0",
    "js-cookie": "^3.0.0",
    "lucide-react": "^0.290.0",
    "next": "^14.0.0",
    "postcss": "^8.4.0",
    "posthog-js": "^1.255.0",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.5.0",
    "react-query": "^3.39.0",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.0",
    "web-vitals": "^3.5.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/react": "18.3.23",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.7.0",
    "cssnano": "^7.0.7",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.0",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
