              return setIsLightboxOpen(false);
            },
            className: "absolute top-4 right-4 bg-white/20 backdrop-blur-sm text-white p-2 rounded-full hover:bg-white/30 transition-colors z-10",
            "aria-label": "Close lightbox",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.X, {
              className: "w-6 h-6"
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "relative w-full h-full max-w-3xl max-h-3xl",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_image.default, {
              src: images[selectedImage],
              alt: `${productName} - Lightbox view`,
              fill: true,
              className: "object-contain",
              sizes: "100vw",
              quality: 100
            })
          }),
          /* istanbul ignore next */
          (cov_1jhnvhar0e().b[27][0]++, images.length > 1) &&
          /* istanbul ignore next */
          (cov_1jhnvhar0e().b[27][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)(_jsxruntime.Fragment, {
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("button", {
              onClick: handlePreviousImage,
              className: "absolute left-4 top-1/2 transform -translate-y-1/2 bg-white/20 backdrop-blur-sm text-white p-3 rounded-full hover:bg-white/30 transition-colors",
              "aria-label": "Previous image",
              children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ChevronLeft, {
                className: "w-6 h-6"
              })
            }), /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
              onClick: handleNextImage,
              className: "absolute right-4 top-1/2 transform -translate-y-1/2 bg-white/20 backdrop-blur-sm text-white p-3 rounded-full hover:bg-white/30 transition-colors",
              "aria-label": "Next image",
              children: /*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.ChevronRight, {
                className: "w-6 h-6"
              })
            })]
          })),
          /* istanbul ignore next */
          (cov_1jhnvhar0e().b[28][0]++, images.length > 1) &&
          /* istanbul ignore next */
          (cov_1jhnvhar0e().b[28][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "absolute bottom-4 left-1/2 transform -translate-x-1/2 bg-white/20 backdrop-blur-sm text-white px-4 py-2 rounded-full",
            children: [selectedImage + 1, " of ", images.length]
          }))]
        })
      }))
    })]
  });
}
