    /* istanbul ignore next */
    cov_1y9n8bnw7y().s[52]++;
    console.log(`âœ… App Router serving: ${pathname}`);
    /* istanbul ignore next */
    cov_1y9n8bnw7y().s[53]++;
    return response;
  } else
  /* istanbul ignore next */
  {
    cov_1y9n8bnw7y().b[11][1]++;
  }
  // Default response with security headers
  const response =
  /* istanbul ignore next */
  (cov_1y9n8bnw7y().s[54]++, _server.NextResponse.next());
  // Add security headers to all responses
  /* istanbul ignore next */
  cov_1y9n8bnw7y().s[55]++;
  response.headers.set("X-Frame-Options", "DENY");
  /* istanbul ignore next */
  cov_1y9n8bnw7y().s[56]++;
  response.headers.set("X-Content-Type-Options", "nosniff");
  /* istanbul ignore next */
  cov_1y9n8bnw7y().s[57]++;
  response.headers.set("Referrer-Policy", "strict-origin-when-cross-origin");
  /* istanbul ignore next */
  cov_1y9n8bnw7y().s[58]++;
  response.headers.set("Permissions-Policy", "camera=(), microphone=(), geolocation=()");
  // Add CSRF protection for form submissions
  /* istanbul ignore next */
  cov_1y9n8bnw7y().s[59]++;
  if (request.method === "POST") {
    /* istanbul ignore next */
    cov_1y9n8bnw7y().b[12][0]++;
    cov_1y9n8bnw7y().s[60]++;
    response.headers.set("X-CSRF-Protection", "1");
  } else
  /* istanbul ignore next */
  {
    cov_1y9n8bnw7y().b[12][1]++;
  }
  cov_1y9n8bnw7y().s[61]++;
  return response;
}
