      /* istanbul ignore next */
      (cov_1c6g1g13r7().b[15][2]++, desc.set))) {
        /* istanbul ignore next */
        cov_1c6g1g13r7().b[14][0]++;
        cov_1c6g1g13r7().s[33]++;
        Object.defineProperty(newObj, key, desc);
      } else {
        /* istanbul ignore next */
        cov_1c6g1g13r7().b[14][1]++;
        cov_1c6g1g13r7().s[34]++;
        newObj[key] = obj[key];
      }
    } else
    /* istanbul ignore next */
    {
      cov_1c6g1g13r7().b[11][1]++;
    }
  }
  /* istanbul ignore next */
  cov_1c6g1g13r7().s[35]++;
  newObj.default = obj;
  /* istanbul ignore next */
  cov_1c6g1g13r7().s[36]++;
  if (cache) {
    /* istanbul ignore next */
    cov_1c6g1g13r7().b[16][0]++;
    cov_1c6g1g13r7().s[37]++;
    cache.set(obj, newObj);
  } else
  /* istanbul ignore next */
  {
    cov_1c6g1g13r7().b[16][1]++;
  }
  cov_1c6g1g13r7().s[38]++;
  return newObj;
}
