  (cov_1c6g1g13r7().b[5][2]++, obj.__esModule)) {
    /* istanbul ignore next */
    cov_1c6g1g13r7().b[4][0]++;
    cov_1c6g1g13r7().s[21]++;
    return obj;
  } else
  /* istanbul ignore next */
  {
    cov_1c6g1g13r7().b[4][1]++;
  }
  cov_1c6g1g13r7().s[22]++;
  if (
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[7][0]++, obj === null) ||
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[7][1]++, typeof obj !== "object") &&
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[7][2]++, typeof obj !== "function")) {
    /* istanbul ignore next */
    cov_1c6g1g13r7().b[6][0]++;
    cov_1c6g1g13r7().s[23]++;
    return {
      default: obj
    };
  } else
  /* istanbul ignore next */
  {
    cov_1c6g1g13r7().b[6][1]++;
  }
  var cache =
  /* istanbul ignore next */
  (cov_1c6g1g13r7().s[24]++, _getRequireWildcardCache(nodeInterop));
  /* istanbul ignore next */
  cov_1c6g1g13r7().s[25]++;
  if (
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[9][0]++, cache) &&
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[9][1]++, cache.has(obj))) {
    /* istanbul ignore next */
    cov_1c6g1g13r7().b[8][0]++;
    cov_1c6g1g13r7().s[26]++;
    return cache.get(obj);
  } else
  /* istanbul ignore next */
  {
    cov_1c6g1g13r7().b[8][1]++;
  }
  var newObj =
  /* istanbul ignore next */
  (cov_1c6g1g13r7().s[27]++, {
    __proto__: null
  });
  var hasPropertyDescriptor =
  /* istanbul ignore next */
  (cov_1c6g1g13r7().s[28]++,
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[10][0]++, Object.defineProperty) &&
  /* istanbul ignore next */
  (cov_1c6g1g13r7().b[10][1]++, Object.getOwnPropertyDescriptor));
  /* istanbul ignore next */
  cov_1c6g1g13r7().s[29]++;
  for (var key in obj) {
    /* istanbul ignore next */
    cov_1c6g1g13r7().s[30]++;
