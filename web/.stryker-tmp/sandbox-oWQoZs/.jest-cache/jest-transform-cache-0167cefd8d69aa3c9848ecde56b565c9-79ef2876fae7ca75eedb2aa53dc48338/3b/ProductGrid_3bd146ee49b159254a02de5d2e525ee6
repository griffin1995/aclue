    cov_1pupjddcrp().b[6][0]++;
    cov_1pupjddcrp().s[15]++;
    return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
      className: "text-center py-12",
      children: /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
        className: "text-gray-500",
        children: "No products to display"
      })
    });
  } else
  /* istanbul ignore next */
  {
    cov_1pupjddcrp().b[6][1]++;
  }
  cov_1pupjddcrp().s[16]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: `grid ${getGridClasses()} gap-6`,
    children: products.map(product => {
      /* istanbul ignore next */
      cov_1pupjddcrp().f[5]++;
      const productName =
      /* istanbul ignore next */
      (cov_1pupjddcrp().s[17]++,
      /* istanbul ignore next */
      (cov_1pupjddcrp().b[7][0]++, product.title) ||
      /* istanbul ignore next */
      (cov_1pupjddcrp().b[7][1]++, product.name) ||
      /* istanbul ignore next */
      (cov_1pupjddcrp().b[7][2]++, "Product"));
      /* istanbul ignore next */
      cov_1pupjddcrp().s[18]++;
      return /*#__PURE__*/(0, _jsxruntime.jsx)(_link.default, {
        href: `/products/${product.id}`,
        className: "group",
        children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
          className: "bg-white rounded-lg shadow-sm border border-gray-200 overflow-hidden hover:shadow-lg transition-shadow",
          children: [/*#__PURE__*/(0, _jsxruntime.jsx)("div", {
            className: "relative aspect-square bg-gray-100",
            children: /*#__PURE__*/(0, _jsxruntime.jsx)(_image.default, {
              src: product.image_url,
              alt: productName,
              fill: true,
              className: "object-cover group-hover:scale-105 transition-transform duration-300",
              sizes: "(max-width: 640px) 100vw, (max-width: 1024px) 50vw, 25vw"
            })
          }), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "p-4",
            children: [/*#__PURE__*/(0, _jsxruntime.jsx)("h3", {
              className: "font-semibold text-gray-900 line-clamp-2 mb-1 group-hover:text-primary-600 transition-colors",
              children: productName
            }),
            /* istanbul ignore next */
            (cov_1pupjddcrp().b[8][0]++, product.brand) &&
            /* istanbul ignore next */
            (cov_1pupjddcrp().b[8][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-sm text-gray-600 mb-2",
              children: product.brand
            })),
            /* istanbul ignore next */
            (cov_1pupjddcrp().b[9][0]++, showDescription) &&
            /* istanbul ignore next */
            (cov_1pupjddcrp().b[9][1]++, product.description) &&
            /* istanbul ignore next */
            (cov_1pupjddcrp().b[9][2]++, /*#__PURE__*/(0, _jsxruntime.jsx)("p", {
              className: "text-sm text-gray-600 line-clamp-2 mb-3",
              children: product.description
            })), /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
              className: "flex items-center justify-between",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                className: "text-lg font-bold text-gray-900",
                children: formatPrice(product.price, product.currency)
              }),
              /* istanbul ignore next */
              (cov_1pupjddcrp().b[10][0]++, product.rating) &&
              /* istanbul ignore next */
              (cov_1pupjddcrp().b[10][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
                className: "flex items-center gap-1",
                children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Star, {
                  className: "w-4 h-4 text-yellow-400 fill-yellow-400"
                }), /*#__PURE__*/(0, _jsxruntime.jsx)("span", {
                  className: "text-sm font-medium text-gray-900",
                  children: product.rating.toFixed(1)
                })]
              }))]
            })]
          })]
        })
      }, product.id);
    })
  });
}
