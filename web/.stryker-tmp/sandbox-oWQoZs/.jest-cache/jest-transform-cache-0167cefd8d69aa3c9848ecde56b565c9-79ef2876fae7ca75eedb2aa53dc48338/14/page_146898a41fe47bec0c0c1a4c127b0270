async function generateStaticParams() {
  /* istanbul ignore next */
  cov_gaxrfpw5x().f[16]++;
  cov_gaxrfpw5x().s[60]++;
  try {
    const result =
    /* istanbul ignore next */
    (cov_gaxrfpw5x().s[61]++, await (0, _products.getProducts)({
      limit: 10
    }));
    /* istanbul ignore next */
    cov_gaxrfpw5x().s[62]++;
    if (
    /* istanbul ignore next */
    (cov_gaxrfpw5x().b[34][0]++, result.success) &&
    /* istanbul ignore next */
    (cov_gaxrfpw5x().b[34][1]++, result.data)) {
      /* istanbul ignore next */
      cov_gaxrfpw5x().b[33][0]++;
      cov_gaxrfpw5x().s[63]++;
      return result.data.map(product => {
        /* istanbul ignore next */
        cov_gaxrfpw5x().f[17]++;
        cov_gaxrfpw5x().s[64]++;
        return {
          id: product.id
        };
      });
    } else
    /* istanbul ignore next */
    {
      cov_gaxrfpw5x().b[33][1]++;
    }
    cov_gaxrfpw5x().s[65]++;
    return [];
  } catch (error) {
    /* istanbul ignore next */
    cov_gaxrfpw5x().s[66]++;
    console.error("Error generating static params:", error);
    /* istanbul ignore next */
    cov_gaxrfpw5x().s[67]++;
    return [];
  }
}
